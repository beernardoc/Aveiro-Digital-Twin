{
  "swagger": "2.0",
  "info": {
    "title": "Simulation API",
    "version": "1.0.0"
  },
  "paths": {
    "/api/run2D": {
      "post": {
        "description": "Run 2D simulation",
        "parameters": [
          {
            "name": "live",
            "in": "query",
            "description": "Flag to run live simulation",
            "required": false,
            "type": "boolean"
          },
          {
            "name": "realdata",
            "in": "query",
            "description": "Flag to run simulation with real data",
            "required": false,
            "type": "boolean"
          },
          {
            "name": "body",
            "in": "body",
            "description": "Real data for the simulation",
            "required": false,
            "schema": {
              "type": "object",
              "properties": {
                "entities": {
                  "type": "array",
                  "items": {
                    "type": "string"
                  }
                },
                "startTime": {
                  "type": "string"
                },
                "endTime": {
                  "type": "string"
                }
              }
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Simulation started successfully",
            "schema": {
              "type": "object",
              "properties": {
                "message": {
                  "type": "string"
                }
              }
            }
          },
          "500": {
            "description": "Internal server error",
            "schema": {
              "type": "object",
              "properties": {
                "error": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
